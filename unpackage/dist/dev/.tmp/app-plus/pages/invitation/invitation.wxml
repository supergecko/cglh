<view class="redBg" style="{{'background-image:'+('url('+redBg+')')+';'}}"><view class="whiteBg" style="{{'background-image:'+('url('+whiteBg+')')+';'}}"><view class="invitationHead"><view class="invitationHeadText1">已邀请好友</view><view class="invitationHeadText2">{{Invitation.invite_number}}</view><view class="invitationHeadText3">邀请码</view><view class="invitationHeadText4">{{Invitation.incite_code}}</view><view data-event-opts="{{[['tap',[['copyText',['$0'],['Invitation.incite_code']]]]]}}" class="copyBTN" bindtap="__e">复制</view></view><view style="width:645rpx;height:1rpx;background:rgba(220,222,226,1);"></view><view class="invitationMiddle"><view class="invitationMiddleText1">邀请链接</view><view class="invitationMiddleText2">{{Invitation.invite_url}}</view><view data-event-opts="{{[['tap',[['copyText',['$0'],['Invitation.invite_url']]]]]}}" class="copyBTN" bindtap="__e">复制</view></view><view class="invitationFooter"><view class="invitationFooterText1">邀请二维码</view><view class="invitationFooterText2"><image style="width:191rpx;height:191rpx;" src="{{Invitation.qrcodeurl}}"></image></view><view data-event-opts="{{[['tap',[['handleDownloadQrIMg',['$0'],['Invitation.qrcodeurl']]]]]}}" class="invitationFooterText3" bindtap="__e">保存</view></view></view></view>